public class Food
private STring name;
private int gewicht;
    public Food(String name, int gewicht)
    this.name = name;
    this.gewicht = gewicht;

public int brechneBrewnnwert()
    return 0;

----------------------------------------

public class Dessert extends Food
    public Dessert(String name)
        super(name, 200);


public int berechneBrewnnwert()
    if(getName().contains("apfel") "oder" getName().contains("frucht")
        return getGewicht()*2;
    else
        return getGewicht();

