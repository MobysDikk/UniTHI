public void printNotenStatistik(Map<String, Integer> notenliste)

    Set<String> keyset = m.keySet();
    int() i = ("0", "0", "0", "0", "0", "0");
    i(0)
    i(1)
    i(2)
    i(3)
    i(4)
    i(5)

    for(int i = 0; i < keyset.size();i++)
        String key = keyset.get(i);
        i(m.get(key)) = i(m.get(key))+1;
